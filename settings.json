        {
            "command": {
                "action": "swapPane",
                "direction": "up"
            },
            "keys": "ctrl+shift+up"
        },
        {
            "command": {
                "action": "swapPane",
                "direction": "down"
            },
            "keys": "ctrl+shift+down"
        },
        {
            "command": {
                "action": "swapPane",
