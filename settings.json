        {
            "command": {
                "action": "scrollUp"
            },
            "keys": "shift+up"
        },
        {
            "command": {
                "action": "scrollDown"
            },
            "keys": "shift+down"
        },
        {
            "command": "scrollUpPage",
            "keys": "pgup"
        },
        {
            "command": "scrollDownPage",
            "keys": "pgdn"
        },
        {
            "command": "scrollToTop",
            "keys": "ctrl+home"
