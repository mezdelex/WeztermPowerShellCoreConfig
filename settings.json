        "split": "right"
      },
      "keys": "ctrl+alt+right"
    },
    {
      "command": "closePane",
      "keys": "ctrl+w"
    },
    // Panes Resizing
    {
      "command": {
        "action": "resizePane",
        "direction": "up"
      },
      "keys": "ctrl+shift+up"
    },
    {
      "command": {
        "action": "resizePane",
        "direction": "down"
