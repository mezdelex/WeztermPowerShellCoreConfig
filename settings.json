        {
            "command": {
                "action": "resizePane",
                "direction": "up"
            },
            "keys": "ctrl+up"
        },
        {
            "command": {
                "action": "resizePane",
                "direction": "down"
            },
            "keys": "ctrl+down"
        },
        {
            "command": {
                "action": "resizePane",
