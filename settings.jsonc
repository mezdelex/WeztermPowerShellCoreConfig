{
    "actions": [
        // Clipboard Management
        {
            "command": {
                "action": "cut",
                "singleLine": false
            },
            "keys": "ctrl+x"
        },
        {
            "command": {
                "action": "copy",
                "singleLine": false
            },
            "keys": "ctrl+c"
        },
        {
            "command": "paste",
            "keys": "ctrl+v"
        },
        // Panes Management
        {
            "command": {
                "action": "splitPane",
                "split": "up"
            },
            "keys": "ctrl+alt+up"
        },
        {
            "command": {
                "action": "splitPane",
                "split": "down"
            },
            "keys": "ctrl+alt+down"
