        {
            "command": {
                "action": "splitPane",
                "split": "right"
            },
            "keys": "ctrl+alt+right"
        },
        {
            "command": "closePane",
            "keys": "ctrl+w"
        },
        // Panes Resizing
        {
            "command": {
                "action": "resizePane",
                "direction": "up"
            },
            "keys": "alt+shift+up"
        },
        {
            "command": {
                "action": "resizePane",
                "direction": "down"
            },
            "keys": "alt+shift+down"
        },
        {
            "command": {
                "action": "resizePane",
                "direction": "left"
            },
            "keys": "alt+shift+left"
        },
        {
            "command": {
                "action": "resizePane",
                "direction": "right"
            },
            "keys": "alt+shift+right"
        },
        // Panes Scrolling
        {
            "command": {
                "action": "scrollUp"
            },
            "keys": "shift+up"
        },
        {
            "command": {
                "action": "scrollDown"
            },
            "keys": "shift+down"
        },
        {
            "command": "scrollUpPage",
            "keys": "pgup"
        },
        {
            "command": "scrollDownPage",
            "keys": "pgdn"
        },
        {
            "command": "scrollToTop",
            "keys": "ctrl+home"
        },
        {
            "command": "scrollToBottom",
            "keys": "ctrl+end"
        },
        // Panes Selection
        {
            "command": {
                "action": "moveFocus",
                "direction": "up"
            },
            "keys": "alt+up"
        },
        {
            "command": {
                "action": "moveFocus",
                "direction": "down"
            },
            "keys": "alt+down"
        },
        {
            "command": {
                "action": "moveFocus",
                "direction": "left"
            },
            "keys": "alt+left"
        },
        {
            "command": {
                "action": "moveFocus",
                "direction": "right"
            },
            "keys": "alt+right"
        },
        // Settings
        {
            "command": {
                "action": "openSettings"
            },
            "keys": "ctrl+comma"
        },
        {
            "command": {
                "action": "openSettings",
                "target": "defaultsFile"
            },
            "keys": "ctrl+alt+comma"
        },
        // Tabs Management
        {
            "command": {
                "action": "newTab"
            },
            "keys": "ctrl+t"
        },
        {
            "command": {
                "action": "nextTab"
            },
            "keys": "ctrl+tab"
        },
        {
            "command": {
                "action": "prevTab"
            },
            "keys": "ctrl+shift+tab"
        }
    ],
