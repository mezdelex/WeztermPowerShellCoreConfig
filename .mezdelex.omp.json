{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "blocks": [
        {
            "alignment": "left",
            "newline": true,
            "segments": [
                {
                    "foreground": "#F38BA8",
                    "powerline_symbol": "\u276f",
                    "style": "plain",
                    "template": "\uf0e7 ",
                    "type": "root"
                },
                {
                    "foreground": "#89B4FA",
                    "properties": {
                        "folder_icon": "\ue5ff",
                        "folder_separator_icon": "/",
                        "home_icon": "\uf7db",
                        "style": "mixed"
                    },
                    "style": "plain",
                    "template": "{{ .Path }}",
                    "type": "path"
                },
                {
                    "foreground": "#F9E2AF",
                    "foreground_templates": [
                        "{{ if or (.Working.Changed) (.Staging.Changed) }}#F9E2AF{{ end }}",
                        "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#F38BA8{{ end }}",
                        "{{ if gt .Ahead 0 }}#F38BA8{{ end }}",
                        "{{ if gt .Behind 0 }}#F9E2AF{{ end }}"
                    ],
                    "properties": {
                        "fetch_stash_count": true,
                        "fetch_status": true,
                        "fetch_upstream_icon": true,
                        "github_icon": "\uf408 "
                    },
                    "style": "plain",
                    "template": " on {{ .UpstreamIcon }}{{ .HEAD }}{{ if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} <#F38BA8>\uf044 {{ .Working.String }}</>{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} <#F9E2AF>\uf046 {{ .Staging.String }}</>{{ end }}{{ if gt .StashCount 0 }} \uf692 {{ .StashCount }}{{ end }}",
                    "type": "git"
                }
            ],
            "type": "prompt"
        },
        {
            "alignment": "right",
            "segments": [
                {
                    "foreground": "#F5C2E7",
                    "properties": {
                        "display_mode": "files"
                    },
                    "style": "plain",
                    "template": " Angular[\ue753]{{ .Full }}",
                    "type": "angular"
                },
                {
                    "foreground": "#F5C2E7",
                    "properties": {
                        "display_mode": "files"
                    },
                    "style": "plain",
                    "template": " Dotnet[\ue77f]{{ .Full }}",
                    "type": "dotnet"
                },
                {
                    "foreground": "#F5C2E7",
                    "properties": {
                        "display_mode": "files"
                    },
                    "style": "plain",
                    "template": " Flutter[\ue28e]{{ .Full }}",
                    "type": "flutter"
                },
                {
                    "foreground": "#F5C2E7",
                    "properties": {
                        "display_mode": "files"
                    },
                    "style": "plain",
                    "template": " Go[\ue65e]{{ .Full }}",
                    "type": "go"
                },
                {
                    "foreground": "#F5C2E7",
                    "properties": {
                        "display_mode": "files"
                    },
                    "style": "plain",
                    "template": " Haskell[\ue777]{{ .Full }}",
                    "type": "haskell"
                },
                {
                    "foreground": "#F5C2E7",
                    "properties": {
                        "display_mode": "files"
                    },
                    "style": "plain",
                    "template": " Java[\ue738]{{ .Full }}",
                    "type": "java"
                },
                {
                    "foreground": "#F5C2E7",
                    "properties": {
                        "display_mode": "files"
                    },
                    "style": "plain",
                    "template": " Kotlin[\ufa05]{{ .Full }}",
                    "type": "kotlin"
                },
                {
                    "foreground": "#F5C2E7",
                    "properties": {
                        "display_mode": "files"
                    },
                    "style": "plain",
                    "template": " Lua[\ue620]{{ .Full }}",
                    "type": "lua"
                },
                {
                    "foreground": "#F5C2E7",
                    "properties": {
                        "display_mode": "files"
                    },
                    "style": "plain",
                    "template": " Node[\ue718]{{ .Full }}",
                    "type": "node"
                },
                {
                    "foreground": "#F5C2E7",
                    "properties": {
                        "display_mode": "files"
                    },
                    "style": "plain",
                    "template": " PHP[\ue73d]{{ .Full }}",
                    "type": "php"
                },
                {
                    "foreground": "#F5C2E7",
                    "properties": {
                        "display_mode": "files"
                    },
                    "style": "plain",
                    "template": " Python[\ue235]{{ .Full }}",
                    "type": "python"
                },
                {
                    "foreground": "#F5C2E7",
                    "properties": {
                        "display_mode": "files"
                    },
                    "style": "plain",
                    "template": " Rust[\ue7a8]{{ .Full }}",
                    "type": "rust"
                },
                {
                    "foreground": "#94E2D5",
                    "style": "plain",
                    "template": " RAM[\uf85a]{{ (div ((sub .PhysicalTotalMemory .PhysicalFreeMemory)) 1000000000) }}/{{ (div .PhysicalTotalMemory 1000000000) }}GB",
                    "type": "sysinfo"
                }
            ],
            "type": "prompt"
        },
        {
            "alignment": "left",
            "newline": true,
            "segments": [
                {
                    "foreground": "#F9E2AF",
                    "properties": {
                        "time_format": "15:04:05"
                    },
                    "style": "plain",
                    "template": "{{ .CurrentDate | date .Format }} ",
                    "type": "time"
                },
                {
                    "foreground": "#F9E2AF",
                    "properties": {
                        "threshold": 0
                    },
                    "style": "plain",
                    "template": "\ufa1a{{ .FormattedMs }} ",
                    "type": "executiontime"
                },
                {
                    "foreground": "#A6E3A1",
                    "style": "plain",
                    "template": "\u276f",
                    "type": "text"
                }
            ],
            "type": "prompt"
        }
    ],
    "final_space": true,
    "version": 2
}
