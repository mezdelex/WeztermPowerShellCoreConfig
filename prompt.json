{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#E67E80",
          "style": "plain",
          "template": "󱐋 ",
          "type": "root"
        },
        {
          "foreground": "#7FBBB3",
          "options": {
            "folder_icon": "",
            "folder_separator_icon": "󰿟",
            "home_icon": "",
            "style": "mixed"
          },
          "style": "plain",
          "template": "{{ .Path }}",
          "type": "path"
        },
        {
          "foreground": "#A7C080",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#DBBC7F{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#E67E80{{ end }}",
            "{{ if gt .Ahead 0 }}#E67E80{{ end }}",
            "{{ if gt .Behind 0 }}#DBBC7F{{ end }}"
          ],
          "options": {
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "plain",
          "template": " on {{ .UpstreamIcon }} {{ .HEAD }}{{ if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} <#E67E80> {{ .Working.String }}</>{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} <#DBBC7F> {{ .Staging.String }}</>{{ end }}{{ if gt .StashCount 0 }} 󰆓 {{ .StashCount }}{{ end }}",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "foreground": "#D699B6",
          "options": {
            "display_mode": "files"
          },
          "style": "plain",
          "template": " .NET[]{{ .Full }}",
          "type": "dotnet"
        },
        {
          "foreground": "#D699B6",
          "options": {
            "display_mode": "files"
          },
          "style": "plain",
          "template": " Angular[]{{ .Full }}",
          "type": "angular"
        },
        {
          "foreground": "#D699B6",
          "options": {
            "display_mode": "files"
          },
          "style": "plain",
          "template": " Go[󰟓]{{ .Full }}",
          "type": "go"
        },
        {
          "foreground": "#D699B6",
          "options": {
            "display_mode": "files"
          },
          "style": "plain",
          "template": " Lua[]{{ .Full }}",
          "type": "lua"
        },
        {
          "foreground": "#D699B6",
          "options": {
            "display_mode": "files"
          },
          "style": "plain",
          "template": " Node[󰎙]{{ .Full }}",
          "type": "node"
        },
        {
          "foreground": "#D699B6",
          "options": {
            "display_mode": "files"
          },
          "style": "plain",
          "template": " Python[]{{ .Full }}",
          "type": "python"
        },
        {
          "foreground": "#D699B6",
          "options": {
            "display_mode": "files"
          },
          "style": "plain",
          "template": " React[]{{ .Full }}",
          "type": "react"
        },
        {
          "foreground": "#D699B6",
          "options": {
            "display_mode": "files"
          },
          "style": "plain",
          "template": " Rust[]{{ .Full }}",
          "type": "rust"
        },
        {
          "foreground": "#D699B6",
          "options": {
            "display_mode": "files"
          },
          "style": "plain",
          "template": " Zig[]{{ .Full }}",
          "type": "zig"
        },
        {
          "foreground": "#83C092",
          "style": "plain",
          "template": " RAM[󰍛]{{ (div ((sub .PhysicalTotalMemory .PhysicalFreeMemory)) 1000000000) }}/{{ (div .PhysicalTotalMemory 1000000000) }}GB",
          "type": "sysinfo"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#DBBC7F",
          "options": {
            "time_format": "15:04:05"
          },
          "style": "plain",
          "template": "{{ .CurrentDate | date .Format }} ",
          "type": "time"
        },
        {
          "foreground": "#DBBC7F",
          "options": {
            "threshold": 0
          },
          "style": "plain",
          "template": "󰔛 {{ .FormattedMs }} ",
          "type": "executiontime"
        },
        {
          "foreground": "#7FBBB3",
          "style": "plain",
          "template": "",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 3
}
